            <field name='res_model'>stock.body</field>
            <field name='view_type'>form</field>
            <field name='view_mode'>tree,form</field>
        </record>
        
        <menuitem id="stock_body_menu" name="Stock Body" parent="stock.menu_stock_configuration" action='action_stock_body' groups="stock.group_production_lot"/>
        
        
