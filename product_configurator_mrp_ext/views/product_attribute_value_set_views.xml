<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_attribute_value_set_form_view" model="ir.ui.view">
        <field name="name">product.attribute.value.set.form.view</field>
        <field name="model">product.attribute.value.set</field>
        <field name="arch" type="xml">
            <form string="Attribute Set">
                <sheet>
                    <group>
                        <group>
                            <field name="config_step_id"/>
                        </group>
                        <group>
                            <field name="value_ids"
                                   options="{'no_create': True, 'no_create_edit': True}"/>
                        </group>
                    </group>
                    <group>
                        <field name="product_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
